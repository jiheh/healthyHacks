{
  "manifest_version": 2,

  "name": "Wildfire News",
  "description": "This extension shows which news sites are reliable",
  "version": "1.0.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "app.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}